{
	"version": "0.1.0",
	"configurations": [
		{
			"type": "node",
			"request": "attach",
			"name": "Attach Debug",
			"protocol": "inspector",
			"port": 9229,
			"restart": true,
			"cwd": "${workspaceRoot}"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Debug",
			"protocol": "inspector",
			"cwd": "${workspaceRoot}",
			"runtimeExecutable": "npm",
			"runtimeArgs": ["run-script", "dev"]
		}
	]
}
